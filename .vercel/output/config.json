{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/contact\\/mailgun\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/contact\\/postmark\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/contact\\/slack\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/newsletter\\/mailchimp\\/?$",
			"dest": "_render"
		}
	]
}